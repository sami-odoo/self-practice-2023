<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="design_report">
        <t t-foreach="docs" t-as="work">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>
                            <span t-field="work.name"/>
                        </h2>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>